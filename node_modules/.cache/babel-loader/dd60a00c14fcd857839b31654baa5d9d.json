ext\" placeholder=\"Описание рецепта\" v-model=\"description\">\n      </div>\n    </div>\n\n    <div class=\"buttons\">\n      <button class=\"btn\" type=\"submit\" :disabled=\"!valid\">Создать</button>\n      <button class=\"btn secondary\" type=\"button\" @click=\"toggle\">\n        {{ visible ? 'Убрать' : 'Показать' }} форму</button>\n    </div>\n  </form>\n</template>\n\n<script>\nexport default {\n  props: {\n    onAdd: {\n      type: Function,\n      required: true\n    }\n  },\n  data() {\n    return {\n      title: '',\n      description: '',\n      visible: true,\n    }\n  },\n  methods: {\n    toggle() {\n      this.visible = !this.visible\n    },\n    submit() {\n      const recipe = {\n        title: this.title.trim(),\n        description: this.description.trim(),\n        id: Date.now().toString()\n      }\n      this.title = this.description = '';\n      this.onAdd(recipe);\n    }\n  },\n  computed: {\n    valid() {\n      return this.title.trim() && this.description.trim();\n    }\n  }\n}\n</script>\n\n<style>\n  .form {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 1rem;\n    border: 1px solid #eee;\n    margin-bottom: 1rem;\n  }\n  .form h1 {\n    margin: 0;\n    margin-bottom: 1rem;\n  }\n  .input {\n    margin-bottom: 1rem;\n  }\n  .input input {\n    border: 1px solid #ccc;\n    border-radius: 5px;\n    padding: 5px 8px;\n    width: 400px;\n  }\n  .buttons {\n    width: 400px;\n    display: flex;\n    justify-content: space-around;\n  }\n</style>"],"sourceRoot":""}]}